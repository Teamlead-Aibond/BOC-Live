<html xmlns="http://www.w3.org/1999/xhtml">

  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>PDF</title>
    <style type="text/css">
      @import url(http://fonts.googleapis.com/css?family=Roboto:400, 100,
      100italic, 300, 300ita‌​lic, 400italic, 500, 500italic, 700, 700italic,
      900italic, 900); html { -webkit-print-color-adjust: exact; } html, body,
      html * { font-family: 'Roboto', sans-serif; } * { margin: 10px 20px 10px
      10px; padding: 0; } .pdf-outer { background-color: #fff; background-clip:
      border-box; padding: 0 0 32px; margin-bottom: 24px; border-radius:
      0.25rem; width: 100%; float: left; } .pdfwrap { padding: 15px 2% 0px 1%;
      margin: 0; float: left; width: 97%; } .pdfheader { width: 100%; float:
      left; margin: 0; padding: 0; } .pdf-content { width: 100%; float: left;
      margin: 0; padding: 0 0 0 12px !important; } .pdfheader-left { width: 25%;
      float: left; margin: 0; padding: 0; } .pdfheader-center { width: 45%;
      float: left; margin: 0; padding: 0; } .pdfheader-right { width: 28%;
      float: left; margin: 0; padding: 0px 3px; text-align: center; }
      .pdf-equal-cell { width: 32.1%; float: left; margin: 0 15px 0 0; padding:
      0; } .pdf-equal-cell h5 { font-size: 11px; margin: 10px 15px; font-weight:
      500; font-family: "Roboto", sans-serif; color: #000; padding: 0; }
      .pdf-equal-cell p { margin: 10px 15px 14px; padding: 0; font-size: 11px;
      font-family: "Roboto", sans-serif; color: #000; line-height: 18px; }
      .table-responsive { display: block; width: 100%; margin: 0; overflow:
      visible; -webkit-overflow-scrolling: touch; } .pdf-content.table { width:
      100%; margin-bottom: 1.5rem; color: #6c757d; border: 1px solid #dee2e6
      !important; border-collapse: collapse; } .pdf-content .print-table tbody {
      border: 1px solid #dee2e6 !important; width: 100% !important; padding: 0
      !important; display: table; margin: 0 !important; border-top: 0
      !important; margin-top: -2px !important; } table.table.print-table {
      width: 100%; margin: 0 !important; padding: 0 !important; } .pdf-content
      .print-table2 tbody { border: 1px solid #dee2e6 !important; width: 100%
      !important; padding: 0 !important; margin: 0 !important; border-top: 0
      !important; margin-top: -2px !important; } table.table.print-table2 {
      width: 98.5%; margin: 0 !important; padding: 0 !important; }
      table.table.print-table2 td { padding: 8px; line-height: 23px; font-size:
      11px; border-right: 1px solid #dee2e6; } table.table.print-table2 td .desc
      { font-size: 10px; } .pdfrow { display: flex; flex-wrap: wrap;
      margin-right: 0px; margin-left: 0; padding-bottom: 20px; float: left;
      width: 100%; } .pdfrow label { color: #000; font-weight: 500; font-size:
      11px; font-family: "Roboto", sans-serif; line-height: 18px; padding: 0 0 0
      2px; margin: 0; } .w-25 { width: 25%; float: left; margin: -2px
      !important; padding: 0px !important; } .w-25 p { font-size: 11px; color:
      #000; margin: 10px 0 0 2px; padding-bottom: 5px; font-family: "Roboto",
      sans-serif; line-height: 19px; padding-top: 0px; } .w-50 { width: 47%;
      float: left; margin: 0 !important; padding: 0px !important; } .w-50 p {
      font-size: 11px; color: #000; margin: 10px 0 0 20px; padding-bottom: 5px;
      font-family: "Roboto", sans-serif; line-height: 19px; padding-top: 0px; }
      .borderblack { border-top: 1px solid #000; margin: -10px 18px 0 0px
      !important; padding: 0; height: 5px; float: left; } table.po-detail {
      text-align: left; padding: 0 !important; margin: 0 !important; width:
      100%; } .nopad { padding: 0 !important } table.po-detail th { font-size:
      11px; font-weight: 500; margin: 0; padding: 5px; color: #000; } table {
      border-collapse: collapse !important; } .noteswrap { margin: 0; padding:
      0; } .noteswrap h6 { font-size: 11px; color: #000; font-family: "Roboto",
      sans-serif; } .noteswrap p { font-size: 11px; } .ccs h3 { font-size: 13px;
      line-height: 20px; color: #000; padding: 0; margin: 0 0 2px; } .ccs p {
      font-size: 11px; line-height: 18px !important; color: #000;
      padding-bottom: 5px; margin: 0; font-family: "Roboto", sans-serif
      !important; } .ccs h2 { font-size: 13px; font-weight: bold; line-height:
      20px; color: #000; padding: 0; margin: 0 0 2px; } .ccs h5 { font-size:
      11px; font-weight: 600; line-height: 18px; color: #000; padding: 0
      !important; margin: 0 !important; } .pdfheader-center h3 { text-align:
      center; } .mt45 { margin-top: 45px; } .addbrd { border: 1px solid #dee2e6;
      float: left; padding: 1%; width: 98%; margin: 0; } .addbrd h3 { font-size:
      13px; line-height: 20px; color: #000; padding: 0; margin: 0 0 2px; }
      .addbrd p { font-size: 11px; line-height: 18px !important; color: #000;
      padding-bottom: 5px; margin: 0; font-family: "Roboto", sans-serif
      !important; } .mt12 { margin-top: 20px; width: 50%; float: right; }
      .mt12_qr { margin-top: 20px; width: 50%; } table.table.carinfo td {
      border-top: 0; border-bottom: 1px solid #dee2e6; padding-bottom: 15px; }
      .carinfo p { padding: 0; margin: 0; } .fw100 { width: 49%; }
      .pdf-splitequal { width: 50%; float: left; margin: 0; padding: 0; }
      .pdf-content .table .thead-light th { color: #FFFFFF !important;
      background-color: #38414A !important; border-color: #38414A !important;
      vertical-align: middle; border-bottom: 2px solid #38414A !important;
      text-align: left; padding: 8px; border: 1px solid #38414A; font-size:
      11px; } .pdf-content .thead-light { border: 1px solid #38414A !important;
      }
    </style>
  </head>

  <body>
    <div class="pdf-outer">
      <div class="pdfwrap">
        <div class="pdfheader">
          <div class="pdfheader-left">
            <img class="logo" src="{{Logo}}" width="180" />
            <div class="ccs">
              <h3>Aibond</h3>
              <p>{{AHAddressList.StreetAddress}},
                {{AHAddressList.SuiteOrApt}},
                {{AHAddressList.City}},
                <br />{{AHAddressList.StateName}},
                {{AHAddressList.CountryName}}
                -
                {{AHAddressList.Zip}}<br />PH:
                {{AHAddressList.PhoneNoPrimary}}
                <br />{{AHAddressList.Fax}}
              </p>
            </div>
          </div>

          <div class="pdfheader-center">
            <h3>
              {{Title}}</h3>
          </div>

          <div class="pdfheader-right">
            {{{BarcodeSVG}}}
            {{!-- <b style="text-align: center;">{{RRInfo.RRNo}}</b> --}}
          </div>
        </div>

        <div class="pdf-content">

          <div class="pdfrow">
            <div style="width:49%; margin-right:1%;" class="pdf-splitequal">
              <div class="table-responsive">
                <table class="table table-bordered print-table">
                  <thead class="thead-light">
                    <tr>
                      <th colspan="2">Ship To:</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td>
                        <h3
                          style="font-size:13px;"
                        >{{RRShippingHistory.ShipToName}}</h3>
                        <p style="font-size:11px;">
                          {{ShipToAddress.StreetAddress}}
                          {{ShipToAddress.SuiteOrApt}}<br
                          />{{ShipToAddress.City}},
                          {{ShipToAddress.StateName}}<br />
                          {{ShipToAddress.CountryName}}
                          -
                          {{ShipToAddress.Zip}}

                        </p>
                      </td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
            {{#ifCond RRShippingHistory.ShowCustomerReference ">" 0}}
              <div style="width:49%; margin-left:1%;" class="pdf-splitequal">
                <div class="table-responsive">
                  <table class="table table-bordered print-table">
                    <thead class="thead-light">
                      <tr>

                        <th colspan="2">Customer Reference:</th>

                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td>

                          {{#each CustomerRef}}
                            <div class="fw100">
                              <div class="row">
                                <p
                                  style="margin-left: 0px;font-size:11px;"
                                >{{ReferenceLabelName}}
                                  :
                                  {{ReferenceValue}}</p>
                              </div>
                            </div>
                          {{/each}}

                        </td>
                      </tr>
                    </tbody>
                  </table>
                </div>
              </div>
            {{/ifCond}}
          </div>

          <div style="margin-top:25px; float:left;" class="table-responsive">
            <table class="table table-bordered print-table2">
              <thead class="thead-light">
                <tr>
                  <th>Req.Ship Date</th>
                  <th>Shipping Terms</th>
                  <th>Ship Via</th>
                  <th>Shipping Account #</th>
                  <th>Tracking #</th>
                  <th>Order Num</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td>{{formatDate RRShippingHistory.ShipDate}}</td>
                  <td>Prepaid & Billed</td>
                  <td>{{RRShippingHistory.ShipViaName}}</td>
                  <td>{{RRShippingHistory.ShippingAccountNo}}</td>
                  <td>{{RRShippingHistory.TrackingNo}}</td>
                  <td>{{RRInfo.RRNo}}</td>
                </tr>
              </tbody>
            </table>

            <div class="table-responsive">
              <table
                class="table mt12_qr carinfo float-left"
                style="float: left; width:45%"
              >
                <tbody>
                  <tr>
                    <td>
                      {{{BarcodeSVGPO}}}
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>

            <div class="table-responsive">
              <table
                class="table mt12_qr carinfo float-right"
                style="float: right; width:45%"
              >
                <tbody>
                  <tr>
                    <td>
                      {{#ifCond
                        RRShippingHistory.ShipToIdentityName
                        "=="
                        "Vendor"
                      }}
                        <p><b>Vendor Reference #</b></p>
                      {{/ifCond}}
                      {{#ifCond
                        RRShippingHistory.ShipToIdentityName
                        "=="
                        "Customer"
                      }}
                        <p><b>Customer PO #</b></p>
                      {{/ifCond}}
                    </td>
                    <td>
                      {{#ifCond
                        RRShippingHistory.ShipToIdentityName
                        "=="
                        "Vendor"
                      }}
                        {{RRShippingHistory.VendorRefNo}}
                      {{/ifCond}}
                      {{#ifCond
                        RRShippingHistory.ShipToIdentityName
                        "=="
                        "Customer"
                      }}
                        {{RRInfo.CustomerPONo}}
                      {{/ifCond}}
                    </td>
                  </tr>
                </tbody>
              </table>
            </div>

          </div>
          {{#ifCond RRShippingHistory.ShowRootCause ">" 0}}
            <div class="pdfrow">
              <div class="noteswrap">
                <h6>Eval Notes:</h6>
                <p><br />{{RRShippingHistory.RootCause}}</p>
              </div>
            </div>
          {{/ifCond}}

          <div style="padding-bottom:0;" class="pdfrow ccs">
            <h5>Shipped Items</h5>

            <div style="padding-top:15px; float:left;" class="table-responsive">
              <table class="table table-bordered print-table2">
                <thead class="thead-light">
                  <tr>
                    <th width="10%">Line #</th>
                    <th width="58%">Item/Description</th>
                    <th width="12%">Serial #</th>
                    <th width="12%">Lead Time</th>
                    <th width="8%">Qty</th>
                  </tr>
                </thead>
                <tbody>
                  {{#each PartItem}}
                    <tr>
                      <td>{{RowIndex}}</td>
                      <td><b>{{PartNo}}</b>
                        {{#ifCond Description "!=" ""}}
                          <br />{{Description}}
                        {{/ifCond}}
                        {{#ifCond SerialNo "!=" null}}
                          {{#ifCond SerialNo "!=" ""}}
                            <br />Serial No:
                            {{SerialNo}}
                          {{/ifCond}}
                        {{/ifCond}}
                      </td>
                      <td>{{SerialNo}}</td>
                      <td>{{LeadTime}}</td>
                      <td>{{Quantity}}</td>
                    </tr>
                  {{/each}}
                </tbody>
              </table>
            </div>
          </div>

          <div class="pdfrow">
            <div class="noteswrap">
              <h6>Customer Stated Issue:</h6>
              {{#ifCond RRInfo.StatedIssue "!=" ""}}
                <p><br />{{RRInfo.StatedIssue}}</p>
              {{/ifCond}}
              {{#ifCond RRInfo.StatedIssue "==" ""}}
                <p><br />NA</p>
              {{/ifCond}}
            </div>
          </div>

          <div class="pdfrow">
            <div class="noteswrap">
              <h6>Shipping Notes:</h6>
              {{#ifCond RRShippingHistory.ShipToIdentityName "==" "Customer"}}
                {{#each CustomerNote}}
                  <p>
                    {{Notes}}
                  </p>
                {{/each}}
              {{/ifCond}}
              {{#ifCond RRShippingHistory.ShipToIdentityName "==" "Vendor"}}
                {{#each RRVendorNote}}
                  <p>
                    {{Notes}}
                  </p>
                {{/each}}
              {{/ifCond}}
              {{#ifCond RRShippingHistory.ShipComment "!=" ""}}
                <p>
                  {{RRShippingHistory.ShipComment}}
                </p>
              {{/ifCond}}
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>

</html>